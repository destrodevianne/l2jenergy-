          </xs:complexType>
        </xs:element>
        <xs:element name="EventMageBuffs">
          <xs:complexType>
            <xs:attribute name="val" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventFighterBuffs">
          <xs:complexType>
            <xs:attribute name="val" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventRestrictItems">
          <xs:complexType>
            <xs:attribute name="val" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventRestrictSkills">
          <xs:complexType>
            <xs:attribute name="val" type="xs:string" use="required" />
