            <xs:attribute name="val" type="xs:boolean" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventTargetTeamMembersAllowed">
          <xs:complexType>
            <xs:attribute name="val" type="xs:boolean" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventRequireMinFragsToReward">
          <xs:complexType>
            <xs:attribute name="val" type="xs:boolean" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="EventMinFragsRequired">
          <xs:complexType>
            <xs:attribute name="val" type="xs:unsignedByte" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>